		<visual-constraints>
			<bendpoints/>
		</visual-constraints>
	</connection>



<!-- (Snowman) --><step id="Snowman" className="com.ataccama.dqc.tasks.expressions.ColumnAssigner" disabled="false" mode="NORMAL">
		<properties>
			<assignments>
				<assignment expression="&#39;â˜ƒ&#39;+tmp_order" column="tmp_order">
					<scorer>
						<scoringEntries/>
					</scorer>
				</assignment>
			</assignments>
		</properties>
		<visual-constraints bounds="168,576,-1,-1" layout="vertical"/>
	</step>
	<connection className="com.ataccama.dqc.model.elements.connections.StandardFlowConnection" disabled="false">
		<source step="Snowman" endpoint="out"/>
